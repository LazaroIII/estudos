		function FormatAmountDec(obj, decimals){
			var numVal

			numVal = obj.value;
			if (isNaN(numVal)){ numVal= 0};
			if (numVal==''){ numVal= 0};

			var strNum = new String(numVal);
			var seq = strNum.split('.');

			if(seq[0]== '') seq[0]= 0;
			var whole = parseFloat(seq[0]);
			var result = '';

			if(seq.length > 1 && seq[1] != ''){
				var dec = new String(seq[1]);
				dec = String(parseFloat(seq[1])/Math.pow(10,(dec.length - decimals)));
				dec = String(whole + Math.round(parseFloat(dec))/Math.pow(10,decimals));
				var dot = dec.indexOf('.');
				if(dot == -1){
					dec += '.';
					dot = dec.indexOf('.');
				}
				while(dec.length <= dot + decimals) { dec += '0'; }
				result = dec;
			}else{
				var dot;
				var dec = new String(whole);
				dec += '.';
				dot = dec.indexOf('.');
				while(dec.length <= dot + decimals) { dec += '0'; }
				result = dec;
			}
			obj.value = result;
			return result;
		}

		function FormatAmountDecV2(obj, decimals)
		{
			var amount = obj.value.trim();
			var dec = isNaN(decimals) || decimals === '' || decimals === null ? 2 : decimals;

			obj.value = isNaN(amount) || amount === '' || amount === null ? parseFloat(0).toFixed(dec) : parseFloat(amount).toFixed(dec);

			return obj.value;
		}

