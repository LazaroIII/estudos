function ValidateCvdField(cvdObj, allowEmptyCvd){
	var cvdLength, cvdLengthTotal = 0;
	//Requires include of javascript_IsInteger.html and javascript_IsNumeric.html
	//Requires vbScript include of VbScript_print_text.asp
	
	//Test if object exists before attempting validity test.
	if(cvdObj){
		//Ensure field is not disabled.
		if (cvdObj.disabled === false) {
			//Validate length of cvd value, should be either 3 or 4 digits.
			cvdLength = cvdObj.value.replace(/^\s+|\s+$/g,"").length;
			cvdLengthTotal = cvdObj.value.length;
			
			if (!(allowEmptyCvd && cvdLengthTotal === 0)){
				if (IsInteger(cvdObj.value) === false || IsPosNumeric(cvdObj.value) === false || (cvdLength < 3 || cvdLength > 4) || cvdLength != cvdLengthTotal) {
					alert("Invalid card CVD");
					cvdObj.focus();
					cvdObj.select();
					cvdObj.value = '';
					return false;
				}else {
					return true;
				}
			}else {
				//If allow empty cvd and none is provided, do not validate the cvd.
				return true;
			}
		}else {
			//Field is disabled or not required, do not attempt to validate field.
			return true;
		}
	}else{
		//Field does not exist, do not attempt to validate field.
		return true;
	}
}	$.fn.showProvinceOptionGroup = function(groupId) {
 		var groupExists = false;
		
		//Remove disabled from "select state/province" option so can be defaulted.
		$(this).find("option[value='']").removeAttr("disabled");
		
		$(this).children().each(function(){
			if($(this).attr("id") != ''){
				if ($(this).attr("id") != groupId){
					$(this).find(":selected").removeAttr("selected");
					
					$(this).attr("disabled", "disabled").hide();
					$(this).children().attr("disabled", "disabled").hide();
				}else{
					$(this).removeAttr("disabled").show();
					$(this).children().removeAttr("disabled").show();		
					groupExists = true;
				}
			}	
 		});
		
		if(groupExists){
			$(this).find("option[value='--']").removeAttr("selected").attr("disabled", "disabled").hide();
		}else{
			$(this).find("option[value='--']").removeAttr("disabled").removeAttr("selected").show();	
		}
		
		//set back to disabled "select state/province" option.
		$(this).find("option[value='']").attr("disabled", "disabled");
		
	}
		
	function CountryUpdate(countryObj, provinceObj){
		if(countryObj){
			try{
				var countryId = countryObj.options[countryObj.selectedIndex].value;
			}catch(err){
				var countryId = countryObj.innerHTML;
			}
			
			if(provinceObj){
				$("#" + provinceObj.id).showProvinceOptionGroup(provinceObj.id + '_country_' + countryId);
				
				//work around for IE9. Does not handle remove attribute "selected" properly.
				if(provinceObj.options[provinceObj.selectedIndex].disabled)
					 provinceObj.selectedIndex = 0;
			}
				
			//Set label values if they exist
			DisplayCountryData(countryObj, countryId, "cityLabel");
			DisplayCountryData(countryObj, countryId, "provinceLabel");
			DisplayCountryData(countryObj, countryId, "postalLabel");
			
			//Determine if province should be displayed.
			if(document.getElementById(countryObj.id + "_displayProvince")){
				if(document.getElementById(countryId + "_displayProvince")){		
					DisplayProvinceText(countryObj, countryId,  provinceObj);
				}else{
					DisplayProvinceText(countryObj, "default",  provinceObj);
				}
			}
		}	
	}
	
	function DisplayCountryData(countryObj, countryId, displayId){
		
		if(document.getElementById(countryObj.id + "_" + displayId)){
			if(document.getElementById(countryId + "_" + displayId)){
				document.getElementById(countryObj.id + "_" + displayId).innerHTML = document.getElementById(countryId + "_" + displayId).innerHTML;
			}else{
				document.getElementById(countryObj.id + "_" + displayId).innerHTML = document.getElementById("default_" + displayId).innerHTML;
			}
		}
	}
	
	function DisplayProvinceText(countryObj,countryId,  provinceObj){
		if(document.getElementById(countryId + "_displayProvince").innerHTML === "False"){
			document.getElementById(countryObj.id + "_displayProvince").style.display ='none';
			//Set selected index of province to be outside us/canada since cannot be explicitly set.
			if(provinceObj)
				$("#" + provinceObj.id).find("option[value='--']").attr("selected", "selected");
		}else{
			document.getElementById(countryObj.id + "_displayProvince").style.display ='';
		}
	}
