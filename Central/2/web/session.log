		}		function DisableControl(objName, dControl) {
			var styleColour, stylePriority, eleType, selText;
			
			if(objName){
				//perform disable if obj exists
				if (dControl == true){
					objName.disabled = true;
					styleColour = "#E5E5E5";
				}else {
					objName.disabled = false;
					styleColour = "#FFFFFF";
					if(document.styleSheets){
						if(document.styleSheets.length > 1){
							var mySheet=document.styleSheets[document.styleSheets.length -1];
							var objRule= mySheet.cssRules? mySheet.cssRules: mySheet.rules
							
							switch(objName.type){
								case 'text': 
									eleType= "input"; 
									break;
								case 'select-one': 
								case 'select-multi': 
									eleType= "select"; 
									break;
								default: 
									eleType="input"; 
									break;
							}
							
							//If the stylesheet has conflicting rules and they are equally specific, 
							//then CSS gives priority to the rule that is later in the stylesheet.
							stylePriority= 0;
							for(i= objRule.length -1; i >= 0; i--){
								selText= String(objRule[i].selectorText).toLowerCase().split(' ').join('');
								//An id selector is more specific than a class selector, 
								//which in turn is more specific than a tag selector.
								if(stylePriority < 4 && objRule[i].style.background && selText.indexOf(String('#' + objName.id).toLowerCase().split(' ').join('')) > -1){
									styleColour = objRule[i].style.background;
									stylePriority = 4;
								}else if(stylePriority < 3 && objRule[i].style.background && selText.indexOf(String('.' + objName.className).toLowerCase().split(' ').join('')) > -1){
									styleColour = objRule[i].style.background;
									stylePriority = 3;
								}else if(stylePriority < 2 && objRule[i].style.backgroundColor && selText.indexOf(String('.' + objName.className).toLowerCase().split(' ').join('')) > -1){
									styleColour = objRule[i].style.backgroundColor;
									stylePriority = 2;
								}else if(stylePriority < 1 && objRule[i].style.background && selText.indexOf(eleType) > -1 && selText.indexOf(eleType + '.') == -1 && selText.indexOf(eleType + '#') == -1){
									styleColour = objRule[i].style.background;
									stylePriority= 1;
								}
							}
						}
					}	
				}
				if(objName.style)	
					objName.style.background = styleColour;
			}
		}


